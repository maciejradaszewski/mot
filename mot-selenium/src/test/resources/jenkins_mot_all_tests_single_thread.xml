<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<!-- Match the thread-count to the number of nodes in the Grid -->
<suite name="All Tests" parallel="classes" thread-count="1">
    <test name="All Tests">
        <packages>
            <package name=".*"/>
        </packages>
    </test>
</suite> 
